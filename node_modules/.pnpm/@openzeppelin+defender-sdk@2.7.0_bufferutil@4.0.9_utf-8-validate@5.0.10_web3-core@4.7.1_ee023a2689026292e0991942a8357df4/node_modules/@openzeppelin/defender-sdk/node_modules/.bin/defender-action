#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xuantung/Desktop/code/nft-marketplace/backend/node_modules/.pnpm/@openzeppelin+defender-sdk-action-client@2.7.0/node_modules/@openzeppelin/defender-sdk-action-client/lib/node_modules:/Users/xuantung/Desktop/code/nft-marketplace/backend/node_modules/.pnpm/@openzeppelin+defender-sdk-action-client@2.7.0/node_modules/@openzeppelin/defender-sdk-action-client/node_modules:/Users/xuantung/Desktop/code/nft-marketplace/backend/node_modules/.pnpm/@openzeppelin+defender-sdk-action-client@2.7.0/node_modules/@openzeppelin/node_modules:/Users/xuantung/Desktop/code/nft-marketplace/backend/node_modules/.pnpm/@openzeppelin+defender-sdk-action-client@2.7.0/node_modules:/Users/xuantung/Desktop/code/nft-marketplace/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xuantung/Desktop/code/nft-marketplace/backend/node_modules/.pnpm/@openzeppelin+defender-sdk-action-client@2.7.0/node_modules/@openzeppelin/defender-sdk-action-client/lib/node_modules:/Users/xuantung/Desktop/code/nft-marketplace/backend/node_modules/.pnpm/@openzeppelin+defender-sdk-action-client@2.7.0/node_modules/@openzeppelin/defender-sdk-action-client/node_modules:/Users/xuantung/Desktop/code/nft-marketplace/backend/node_modules/.pnpm/@openzeppelin+defender-sdk-action-client@2.7.0/node_modules/@openzeppelin/node_modules:/Users/xuantung/Desktop/code/nft-marketplace/backend/node_modules/.pnpm/@openzeppelin+defender-sdk-action-client@2.7.0/node_modules:/Users/xuantung/Desktop/code/nft-marketplace/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@openzeppelin+defender-sdk-action-client@2.7.0/node_modules/@openzeppelin/defender-sdk-action-client/lib/bin.js" "$@"
else
  exec node  "$basedir/../../../../../../@openzeppelin+defender-sdk-action-client@2.7.0/node_modules/@openzeppelin/defender-sdk-action-client/lib/bin.js" "$@"
fi
